{
  "name": "tailwind-credit-indicator",
  "version": "0.1.0",
  "description": "A beautiful, animated credit/battery indicator component built with React, TypeScript, and Tailwind CSS",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "keywords": [
    "react",
    "tailwind",
    "credit",
    "battery",
    "indicator",
    "ui",
    "component"
  ],
  "author": "Sagar Gandhi",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "tailwindcss": ">=3.0.0"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lucide-react": "^0.290.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joyspace-ai/public-tailwind-credit-indicator"
  },
  "bugs": {
    "url": "https://github.com/joyspace-ai/public-tailwind-credit-indicator/issues"
  },
  "homepage": "https://github.com/joyspace-ai/public-tailwind-credit-indicator#readme"
}
